<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>servlets.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DonorInfo</servlet-name>
        <servlet-class>servlets.DonorInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HospitalLogin</servlet-name>
        <servlet-class>servlets.HospitalLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>servlets.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrevoiusInfo</servlet-name>
        <servlet-class>servlets.PrevoiusInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileInfo</servlet-name>
        <servlet-class>servlets.ProfileInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>servlets.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DonorInfo</servlet-name>
        <url-pattern>/DonorInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HospitalLogin</servlet-name>
        <url-pattern>/HospitalLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrevoiusInfo</servlet-name>
        <url-pattern>/PrevoiusInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileInfo</servlet-name>
        <url-pattern>/ProfileInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
